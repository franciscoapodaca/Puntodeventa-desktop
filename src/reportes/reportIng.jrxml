	COUNT (di.id_ingrediente) 'INGREDIENTE', nombre
FROM
	DetalleIngrediente di, Ingrediente i, Venta v

WHERE
	v.id_Venta=di.id_Venta AND i.id_ingrediente=di.id_ingrediente
	AND fecha BETWEEN $P{FECHAINICIO} AND $P{FECHAFIN}
GROUP BY
	nombre]]>
